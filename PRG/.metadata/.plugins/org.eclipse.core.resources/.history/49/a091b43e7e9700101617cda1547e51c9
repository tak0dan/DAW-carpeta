package Tema_2;

public class Ej5 {
    public static void main(String[] args) {
        
        //int a, b, c=0;
        int a;
        int b;
        int c = 0;
        
        //a = 5;
        a = 5;
        
        //b = -6+a;
        b = (-6 + a);
        
        //a++;
        a++;
        
        //b = a * 10;
        b = a * 10;
        
        //c --;
        c--;
        
        //c = b;
        c = b;
    }
}
