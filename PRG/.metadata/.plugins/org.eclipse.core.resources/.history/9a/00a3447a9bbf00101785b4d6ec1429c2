package Tema_5;
import java.util.Random;
public class Ej_8 {
	public static void main(String[] args) {
		Random r = new Random(7);
		int aux=0;
		int [] v = {r.nextInt(10),r.nextInt(10),r.nextInt(10),r.nextInt(10),r.nextInt(10),r.nextInt(10),r.nextInt(10),r.nextInt(10)};
		for(int i = 0, j=v.length;i<j;i++,j--) {
			aux = v[i];
			v[i] = v[j];
			v[j]=aux;
		}
		MetodosA.MostrarArray(v);
	}
}
