package Tema_3.Tareas3_4;
import java.util.Scanner;
public class cuadrdvco {
    public static void main(String[] args) {
    	Scanner tec = new Scanner(System.in);
        int FIN = tec.nextInt();

        for (int i = 0; i < FIN; i++) {
            for (int j = 0; j < FIN; j++) {
                if (i == 0 || i == FIN - 1 || j == 0 || j == FIN - 1) {
                    System.out.print("* ");
                } else {
                    System.out.print("  "); 
                }
            }
            System.out.println();
        }
    }
}
